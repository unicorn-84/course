{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*.@(jpg|jpeg|gif|png|webp|svg|css|js)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=3153600"
      } ]
    } ]
  }
}